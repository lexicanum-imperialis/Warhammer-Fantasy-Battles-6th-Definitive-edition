    <selectionEntry type="upgrade" import="true" name="Dark Mace of Death  [CHAOS DWARFS]" hidden="false" id="9dbbb1f8-ecdc-4efe-aaca-2fd9100cae7c" publicationId="2d65676f-0d4a-476c-881b-5ef18e479c89" collective="false">
