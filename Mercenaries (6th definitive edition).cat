                <characteristic name="Effect" typeId="7d21-349e-b0a8-fc7d">Models killed make 1 attack against their own unit</characteristic>
              </characteristics>
            </profile>
            <profile name="3. Nightmare" typeId="07eb-6084-5f84-a505" typeName="Spell" hidden="false" id="f687-8b83-8843-a4b8" page="14" publicationId="91c2-b862-8010-87bb">
              <characteristics>
                <characteristic name="Cast" typeId="f1e6-8816-26e0-8a70">7+</characteristic>
