                <characteristic name="CC" typeId="d360-4f33-c1a6-b3ab"/>
                <characteristic name="Target" typeId="d1af-fdf7-8ed2-d6a3">Enemy</characteristic>
                <characteristic name="Effect" typeId="7d21-349e-b0a8-fc7d">1D6 S5</characteristic>
              </characteristics>
            </profile>
            <profile name="2. Betrayal in Death" typeId="07eb-6084-5f84-a505" typeName="Spell" hidden="false" id="b7b8-0a89-f752-756b" page="14" publicationId="91c2-b862-8010-87bb">
              <characteristics>
                <characteristic name="Cast" typeId="f1e6-8816-26e0-8a70">7+</characteristic>
                <characteristic name="Range" typeId="42e6-553f-842f-0b91">18&quot;</characteristic>
                <characteristic name="Type" typeId="194f-3dee-9698-7291">Curse (until next turn)</characteristic>
                <characteristic name="CC" typeId="d360-4f33-c1a6-b3ab"/>
