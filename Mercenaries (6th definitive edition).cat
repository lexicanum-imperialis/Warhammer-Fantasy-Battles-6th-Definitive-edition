                <characteristic name="Damage" typeId="6f2f-d612-52ef-1633">D3</characteristic>
                <characteristic name="Special Rules" typeId="a21a-cdc0-4b13-b236">Cannon</characteristic>
                <characteristic name="Saves" typeId="8014-a8b2-15c3-da95">No armour saves</characteristic>
              </characteristics>
