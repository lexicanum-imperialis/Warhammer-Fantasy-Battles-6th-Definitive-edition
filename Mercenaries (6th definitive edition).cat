            <profile name="4. Curse of the Dark Master" typeId="07eb-6084-5f84-a505" typeName="Spell" hidden="false" id="948c-77be-ce61-8400" page="14" publicationId="91c2-b862-8010-87bb">
              <characteristics>
                <characteristic name="Cast" typeId="f1e6-8816-26e0-8a70">8+</characteristic>
                <characteristic name="Range" typeId="42e6-553f-842f-0b91">ALL</characteristic>
                <characteristic name="Type" typeId="194f-3dee-9698-7291">Remains in play</characteristic>
                <characteristic name="CC" typeId="d360-4f33-c1a6-b3ab"/>
                <characteristic name="Target" typeId="d1af-fdf7-8ed2-d6a3">Any enemy unit</characteristic>
                <characteristic name="Effect" typeId="7d21-349e-b0a8-fc7d">-1 to hit.</characteristic>
              </characteristics>
            </profile>
            <profile name="5. Fog of Death" typeId="07eb-6084-5f84-a505" typeName="Spell" hidden="false" id="5ee0-1574-7721-e834" page="14" publicationId="91c2-b862-8010-87bb">
              <characteristics>
                <characteristic name="Cast" typeId="f1e6-8816-26e0-8a70">10+</characteristic>
                <characteristic name="Range" typeId="42e6-553f-842f-0b91">ALL</characteristic>
                <characteristic name="Type" typeId="194f-3dee-9698-7291">Magic missile</characteristic>
                <characteristic name="CC" typeId="d360-4f33-c1a6-b3ab"/>
                <characteristic name="Target" typeId="d1af-fdf7-8ed2-d6a3">All enemy units, allied units at 4+</characteristic>
                <characteristic name="Effect" typeId="7d21-349e-b0a8-fc7d">1D6 S3</characteristic>
