    <profile name="Alrik Ranulfsson" typeId="a54a-7f00-29bf-12b1" typeName="Profile" hidden="false" id="3fb8-137c-64e6-de88" page="104" publicationId="66a2-453a-48d1-18e0">
      <characteristics>
        <characteristic name="Mv" typeId="0e92-d038-82bf-fb41">3</characteristic>
