    <profile id="504d-03f5-6794-040f" name="Anvil Guard" hidden="false" typeId="a54a-7f00-29bf-12b1" typeName="Profile">
      <characteristics>
        <characteristic name="Mv" typeId="0e92-d038-82bf-fb41">3</characteristic>
