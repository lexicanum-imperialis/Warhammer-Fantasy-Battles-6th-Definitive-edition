      <profiles>
        <profile id="a075-1955-8d0c-b59c" name="Grey Seer Thanquol" publicationId="f804-741f-2593-2dec" page="66" hidden="false" typeId="a54a-7f00-29bf-12b1" typeName="Profile">
          <characteristics>
            <characteristic name="Mv" typeId="0e92-d038-82bf-fb41">5</characteristic>
            <characteristic name="WS" typeId="f95b-da01-0578-3bdc">3</characteristic>
            <characteristic name="BS" typeId="4a8b-0c8e-3daf-7901">3</characteristic>
