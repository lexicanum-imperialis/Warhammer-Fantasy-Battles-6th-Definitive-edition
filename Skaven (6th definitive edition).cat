    <selectionEntryGroup id="a178-48ca-5c29-af2a" name="Lore of the Horned Rat" publicationId="f804-741f-2593-2dec" page="34" hidden="false" collective="false" import="true">
      <entryLinks>
        <entryLink id="f233-20fe-ee51-5084" name="1. Skitterleap" hidden="false" collective="false" import="true" targetId="b2d7-83c8-c22a-48db" type="selectionEntry"/>
