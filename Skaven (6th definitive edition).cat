        <rule id="8d3e-825b-7b80-b913" name="Leadership (Master Assassin)" publicationId="f804-741f-2593-2dec" page="76" hidden="true">
          <modifiers>
            <modifier type="set" field="hidden" value="false">
              <conditions>
